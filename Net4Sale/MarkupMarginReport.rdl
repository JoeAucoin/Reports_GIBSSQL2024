  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Net4Sale</DataSourceName>
        <CommandText>
SELECT
	 [Id] ,
	 [Sku]
	,	[Name]
  --    ,[ShortDescription]
      --,[FullDescription]
  
      
  

      ,[Price]

      ,[ProductCost]

	  , cast((( ([Price] - [ProductCost]) / [ProductCost] ) ) as decimal(18,2)) AS Markup

	   , cast((( ([Price] - [ProductCost]) / [Price] ) ) as decimal(18,2)) AS Margin

	  --,cast((OrderTotal - [OrderShippingExclTax] - SUM(dbo.OrderItem.Quantity * dbo.OrderItem.OriginalProductCost)) / [OrderTotal] as decimal(18,2))  AS WShipPercent


--      ,[Weight]
 --       ,[DisplayOrder]
 --     ,[Published]
 --     ,[Deleted]
 --     ,[CreatedOnUtc]
 --     ,[UpdatedOnUtc]
  FROM [dbo].[Product]
  WHERE [Deleted] = 0 
  --AND (cast((( ([Price] - [ProductCost]) / [ProductCost] ) * 100) as decimal(18,2)) &lt; 70)
  ORDER BY 
  [Name]</CommandText>
      </Query>
