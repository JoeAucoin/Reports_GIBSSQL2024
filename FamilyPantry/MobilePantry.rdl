  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>inetDNN_FamilyPantry</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--Declare @StartDate date, @EndDate date


	--Set @StartDate = '11/01/2019'
	--Set @EndDate = '11/25/2020'


	SELECT         GIBS_FBClients.ClientID, GIBS_FBClients.ClientFirstName, GIBS_FBClients.ClientLastName, GIBS_FBClients.ClientCity, vw_GIBS_FBVisits.visitdate, vw_GIBS_FBVisits.VisitNumBags
 
FROM         GIBS_FBClients    

INNER JOIN
                         vw_GIBS_FBVisits ON vw_GIBS_FBVisits.clientid = GIBS_FBClients.ClientID 
						 
						 INNER JOIN
                  GIBS_FBClientsTrueFalseQuestions       ON GIBS_FBClientsTrueFalseQuestions.ClientID = GIBS_FBClients.ClientID

				  WHERE    (GIBS_FBClientsTrueFalseQuestions.TfQuestion = 'Mobile Pantry') AND  (visitdate between @StartDate AND @EndDate) 

				  AND (GIBS_FBClientsTrueFalseQuestions.TfAnswer = 1)

				  ORDER BY  GIBS_FBClients.ClientCity, GIBS_FBClients.ClientLastName</CommandText>
      </Query>
